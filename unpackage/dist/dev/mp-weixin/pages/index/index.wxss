.contanier .banner {
  position: relative;
  width: 100%;
  height: 600rpx;
}
.contanier .banner .bg {
  width: 100%;
  height: 600rpx;
}
.contanier .banner .introl {
  position: absolute;
  top: calc(50rpx + 25px);
  left: 40rpx;
  color: #fff;
  display: flex;
  flex-direction: column;
}
.contanier .banner .introl .greet {
  font-size: 32rpx;
}
.contanier .banner .introl .note {
  font-size: 24rpx;
}
.contanier .content {
  padding: 0 30rpx;
}
.contanier .content .entrance {
  position: relative;
  margin-top: -80rpx;
  margin-bottom: 30rpx;
  border-radius: 10rpx;
  background-color: #fff;
  box-shadow: 0 20rpx 20rpx -20rpx rgba(51, 51, 51, 0.1);
  padding: 30rpx 0;
  display: flex;
  justify-content: center;
  align-items: center;
}
.contanier .content .entrance .item {
  flex: 1;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  position: relative;
}
.contanier .content .entrance .item:nth-child(1)::after {
  content: '';
  width: 1rpx;
  height: 100%;
  position: absolute;
  background-color: #ddd;
  right: 0;
  -webkit-transform: scaleX(0.5) scaleY(0.8);
          transform: scaleX(0.5) scaleY(0.8);
}
.contanier .content .entrance .item .icon {
  width: 84rpx;
  height: 84rpx;
  margin: 20rpx;
}
.contanier .content .info {
  position: relative;
  background-color: #fff;
  border-radius: 10rpx;
  margin-bottom: 30rpx;
  box-shadow: 0 20rpx 20rpx -20rpx rgba(51, 51, 51, 0.1);
  padding: 30rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}
.contanier .content .info .left {
  flex: 1;
  display: flex;
  flex-direction: column;
  justify-content: center;
}
.contanier .content .info .left .top {
  display: flex;
  align-items: center;
}
.contanier .content .info .left .top .title {
  color: #5A5B5C;
  font-size: 28rpx;
  margin-right: 10rpx;
}
.contanier .content .info .left .top .value {
  font-size: 44rpx;
  font-weight: bold;
}
.contanier .content .info .left .bottom {
  font-size: 24rpx;
  color: #919293;
  display: flex;
  align-items: center;
}
.contanier .content .info .right {
  font-size: 24rpx;
  color: #ADB838;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}
.contanier .content .info .right .qrcode {
  width: 40rpx;
  height: 40rpx;
  margin-bottom: 10rpx;
}
.contanier .content .nav {
  width: 100%;
  padding: 20rpx;
  margin-bottom: 20rpx;
  border-radius: 10rpx;
  display: flex;
  align-items: stretch;
  background-color: #ffffff;
  box-shadow: 0 20rpx 20rpx -20rpx rgba(51, 51, 51, 0.1);
}
.contanier .content .nav .left {
  width: 340rpx;
  background-color: #F2F2E6;
  margin-right: 20rpx;
  display: flex;
  flex-direction: column;
  padding: 0 20rpx;
  font-size: 24rpx;
  color: #5A5B5C;
}
.contanier .content .nav .left .introl-top {
  display: flex;
  flex-direction: column;
  justify-content: center;
  height: 50%;
}
.contanier .content .nav .left .introl-top .top-box {
  display: flex;
  align-items: center;
}
.contanier .content .nav .right {
  width: 290rpx;
  display: flex;
  flex-direction: column;
}
.contanier .content .nav .right .tea-active, .contanier .content .nav .right .member-gifts {
  width: 100%;
  display: flex;
  padding: 20rpx;
  font-size: 24rpx;
  color: #5A5B5C;
  align-items: center;
  position: relative;
}
.contanier .content .nav .right .tea-active {
  background-color: #FDF3F2;
  margin-bottom: 20rpx;
}
.contanier .content .nav .right .member-gifts {
  background-color: #FCF6D4;
}
.contanier .content .member-news {
  width: 100%;
  margin-bottom: 30rpx;
}
.contanier .content .member-news .header {
  padding: 20rpx 0;
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.contanier .content .member-news .header .title {
  font-size: 32rpx;
  font-weight: bold;
}
.contanier .content .member-news .header .iconfont {
  font-size: 52rpx;
  color: #919293;
}
.contanier .content .member-news .list {
  width: 100%;
  display: flex;
  flex-direction: column;
}
.contanier .content .member-news .list .item {
  height: 240rpx;
  position: relative;
  width: 100%;
}
.contanier .content .member-news .list .item image {
  height: 100%;
  width: 100%;
  border-radius: 8rpx;
}
.contanier .content .member-news .list .item .title {
  position: relative;
  font-weight: 500;
  font-size: 32rpx;
  color: #fff;
  top: -70rpx;
  left: 16rpx;
  width: 100%;
}
.mark-img {
  width: 30rpx;
  height: 30rpx;
  margin-right: 10rpx;
}
.yzclh-img {
  height: 122.96rpx;
  width: 214.86rpx;
}
.right-img {
  flex: 1;
  position: relative;
  margin-left: 20rpx;
  margin-right: -20rpx;
  margin-bottom: -20rpx;
  display: flex;
  align-items: flex-end;
}
.right-img image {
  width: 100%;
}

